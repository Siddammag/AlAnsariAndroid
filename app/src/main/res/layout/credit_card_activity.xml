<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:animateLayoutChanges="true"
    android:background="@color/bg"
    tools:ignore="MissingPrefix">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:orientation="vertical">

        <include layout="@layout/toolbar_actionbar_account_management" />

        <include layout="@layout/search_view_layout" />

        <android.support.v7.widget.AppCompatImageView
            android:id="@+id/fab"
            style="@style/addBeneficiaryButtonNew"
            app:srcCompat="@drawable/ic_add_new" />

        <include layout="@layout/recycler_view_full" />
    </LinearLayout>

    <!--<android.support.v7.widget.AppCompatImageView
        android:id="@+id/fab"
        style="@style/FloatingRoundButton"
        android:src="@drawable/ic_action_add" />-->
</FrameLayout>