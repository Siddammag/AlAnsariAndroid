<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dimens_34"
    android:layout_marginLeft="@dimen/dimens_8"
    android:gravity="center_vertical"
    android:orientation="horizontal"
    android:paddingLeft="5dp"
    tools:ignore="MissingPrefix">

    <ImageView
        android:id="@+id/iv_icon"
        android:layout_width="@dimen/dimens_24"
        android:layout_height="@dimen/dimens_24"
        android:layout_marginRight="@dimen/dimens_8"
        android:padding="@dimen/dimens_3" />

    <TextView
        android:id="@+id/left_menu_item_name"
        fontPath="fonts/Roboto-Light.ttf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/color3F3F3F"
        android:textSize="@dimen/dimens_12sp" />

    <TextView
        android:id="@+id/left_menu_item_app_version"
        fontPath="fonts/Roboto-Light.ttf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/color7F8486"
        android:textSize="@dimen/dimens_10sp" />
</LinearLayout>