<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/splash_bg"></solid>
            <corners android:radius="@dimen/dimens_5"></corners>
            <solid android:color="@color/splash_bg"></solid>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/splash_bg"></solid>
            <corners android:radius="@dimen/dimens_5"></corners>
            <stroke android:width="@dimen/dimens_2" android:color="@color/splash_bg"></stroke>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/splash_bg"></solid>
            <corners android:radius="@dimen/dimens_5"></corners>
            <stroke android:width="@dimen/dimens_2" android:color="@color/splash_bg"></stroke>
        </shape>
    </item>
</selector>