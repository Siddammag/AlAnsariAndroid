<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/colorE6E7E7"></solid>
            <corners android:radius="@dimen/dimens_70"></corners>
            <solid android:color="@color/colorE6E7E7"></solid>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/color62A930"></solid>
            <corners android:radius="@dimen/dimens_70"></corners>
            <stroke android:width="@dimen/dimens_2" android:color="@color/color62A930"></stroke>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/color70D303"></solid>
            <corners android:radius="@dimen/dimens_70"></corners>
            <stroke android:width="@dimen/dimens_2" android:color="@color/color70D303"></stroke>
        </shape>
    </item>
</selector>